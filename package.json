{
  "name": "notneeded",
  "version": "0.0.1",
  "description": "Detects redundant dependencies which should be avoided and replaced with native functionality",
  "main": "lib/cli.js",
  "scripts": {
    "clean": "rimraf ./lib",
    "lint": "eslint \"src/**/*.ts\"",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "mocha \"lib/test/**/*_test.js\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/43081j/notneeded.git"
  },
  "author": "James Garbutt (https://github.com/43081j)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/43081j/notneeded/issues"
  },
  "homepage": "https://github.com/43081j/notneeded#readme",
  "devDependencies": {
    "@types/eslint": "^4.16.6",
    "@types/estree": "0.0.39",
    "@types/node": "^12.0.2",
    "@types/react": "^16.8.18",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "prettier": "^1.17.1",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "ink": "^2.2.0",
    "react": "^16.8.6"
  }
}
